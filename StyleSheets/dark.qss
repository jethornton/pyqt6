QWidget {
background: rgba(0, 0, 0, 60%);
}

QLabel#label_1 {
border: 2px solid rgb(26, 26, 255);
}

QLabel#label_2 {
border: 2px solid rgb(255, 255, 153);
}

QLabel#label_3 {
border: 2px solid rgb(51, 102, 255);
}

QLabel#label_4 {
border: 2px solid rgb(102, 51, 0);
}

QLabel#label_5 {
border: 2px solid rgb(255, 153, 0);
}

QLabel#label_6 {
border: 2px solid rgb(0, 0, 153);
}

QLabel#label_7 {
border: 2px solid rgb(255, 0, 0);
}

QLabel#label_8 {
